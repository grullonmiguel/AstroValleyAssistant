<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">


    <SolidColorBrush po:Freeze="True" x:Key="Brush.Accent" Color="#534B80" />
    <SolidColorBrush po:Freeze="True" x:Key="Brush.Accent.Hover" Color="#3E3175" />

    <SolidColorBrush po:Freeze="True" x:Key="Brush.Background.Primary" Color="#EDEBF2" />
    <SolidColorBrush po:Freeze="True" x:Key="Brush.Background.Secondary" Color="#FFFFFF" />
    <SolidColorBrush po:Freeze="True" x:Key="Brush.Background.Dialog" Color="#80000000" />

    <SolidColorBrush po:Freeze="True" x:Key="Brush.Text.Subtle" Color="#DBD8E9"/>
    <SolidColorBrush po:Freeze="True" x:Key="Brush.Accent.Foreground" Color="#EDEBF2"/>
    <SolidColorBrush po:Freeze="True" x:Key="Brush.Sidebar.Foreground" Color="#DBD8E9"/>

    <SolidColorBrush po:Freeze="True" x:Key="Brush.Pill.Background" Color="#DBD8E9"/>
</ResourceDictionary>